package com.angus.project;

/** 
 * @author GuangSIR 
 * @date 2018年3月17日 下午7:23:54 
 * @version 1.0 
 */

// 抽象组件类：抽象同事类
abstract class Component {
	protected Mediator mediator;
	
	public void setMediator(Mediator mediator){
		this.mediator = mediator;
	}
	
	// 转发调用
	public void changed(){
		mediator.componentChanged(this);
	}
}
